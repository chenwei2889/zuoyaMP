<!--pages/count_login/count_login.wxml-->
<navbar parameter='{{parameter}}'></navbar>
<view class="container">
  <view class="logo">
    <image src="/images/hand.png"></image>
  </view>
  <view class="btnout">
    <button class="btn" hover-class="none" open-type="getPhoneNumber" bindgetphonenumber="getPhoneNumber">微信一键登录</button>
  </view>
  <navigator url="../verifycode_login/verifycode_login" hover-class="none">
    <view class="phonelogin">
      <text>手机号验证码登录</text>
    </view>
  </navigator>
</view>

<authorize bind:onLoadFun="onLoadFun"></authorize>