/* pages/jishi_ticheng_de/index.wxss */
page{
  background-color: #fff!important
}

.list{
  padding-bottom: 50rpx;
}

.item{
  padding: 0 20rpx;
  width: 100%;
  height: 130rpx;
  border-bottom: solid 1rpx #ececec;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.item .left{
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  height: 130rpx;
  padding: 25rpx 0;
  font-size: 24rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
	color: #666666;
}

.item .left view:first-child{
  font-size: 30rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
	color: #333333;
}

.item .right{
  font-family: PingFang-SC-Heavy;
	font-size: 32rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
	color: #333333;
}